# 데이터베이스 연결 정보 (Supabase)
DB_URL=jdbc:postgresql://your-supabase-host:5432/postgres
DB_USERNAME=postgres
DB_PASSWORD=your-password

# CORS 설정 (쉼표로 구분)
# 예시: http://localhost:*,https://your-app.vercel.app,https://your-app-staging.vercel.app
CORS_ALLOWED_ORIGINS=http://localhost:*

# JWT 시크릿 (프로덕션에서는 반드시 변경!)
JWT_SECRET=very-very-secret-key-that-is-at-least-32-bytes

# Redis (선택사항, 없으면 localhost:6379 사용)
# REDIS_HOST=your-redis-host
# REDIS_PORT=6379

# 이메일 설정 (선택사항)
# MAIL_USERNAME=your-email@gmail.com
# MAIL_PASSWORD=your-app-password
